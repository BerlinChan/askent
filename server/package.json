{
  "name": "askent-server",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "postinstall": "npm -s run generate",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly --inspect=9229 src/index",
    "start": "node dist/index",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "prisma:migrate-save": "prisma2 migrate save --name 'init' --experimental",
    "prisma:migrate-up": "prisma2 migrate up --experimental",
    "prisma:generate": "prisma2 generate",
    "prisma:seed": "ts-node prisma/seed",
    "generate": "npm -s run prisma:generate && npm -s run generate:nexus",
    "generate:nexus": "ts-node --transpile-only src/schema"
  },
  "dependencies": {
    "@prisma/client": "2.0.0-preview021",
    "apollo-server-express": "2.10.1",
    "bcryptjs": "2.4.3",
    "date-fns": "2.9.0",
    "dotenv": "8.2.0",
    "graphql": "14.6.0",
    "graphql-middleware": "4.0.2",
    "graphql-shield": "7.0.12",
    "jsonwebtoken": "8.5.1",
    "nexus": "0.12.0-rc.11",
    "nexus-prisma": "0.9.1",
    "ramda": "0.27.0"
  },
  "devDependencies": {
    "@prisma/sdk": "0.0.216",
    "@types/bcryptjs": "2.4.2",
    "@types/express-jwt": "0.0.42",
    "@types/jsonwebtoken": "8.3.7",
    "@types/node": "13.7.1",
    "@types/node-fetch": "2.5.4",
    "@types/ramda": "types/npm-ramda#dist",
    "create-nexus-type": "1.1.7",
    "prisma2": "2.0.0-preview021",
    "ts-node": "8.6.2",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.7.5"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  }
}
