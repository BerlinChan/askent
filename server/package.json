{
  "name": "askent-server",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "postinstall": "npm -s run generate",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly --inspect=9229 src/index",
    "start": "node dist/index",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "prisma:dev": "npx prisma2 dev",
    "prisma:seed": "ts-node prisma/seed",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "npx prisma2 generate",
    "generate:nexus": "ts-node --transpile-only src/schema"
  },
  "dependencies": {
    "@prisma/photon": "2.0.0-preview019",
    "apollo-server-express": "2.9.16",
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "graphql": "14.6.0",
    "graphql-middleware": "4.0.2",
    "graphql-shield": "7.0.9",
    "jsonwebtoken": "8.5.1",
    "nexus": "0.12.0-rc.5",
    "nexus-prisma": "0.6.1",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@prisma/sdk": "0.0.172",
    "@types/bcryptjs": "2.4.2",
    "@types/express-jwt": "0.0.42",
    "@types/jsonwebtoken": "8.3.7",
    "@types/node": "13.7.0",
    "@types/node-fetch": "2.5.4",
    "@types/ramda": "types/npm-ramda#dist",
    "create-nexus-type": "1.1.4",
    "prisma2": "2.0.0-preview019",
    "ts-node": "8.6.2",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.7.5"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  }
}
