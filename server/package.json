{
  "name": "askent-server",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "postinstall": "npm -s run generate",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly --inspect=9229 src/index",
    "start": "node dist/index",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "seed": "npx sequelize db:seed:all --config ./src/db/config.js --models-path ./src/models --seeders-path ./src/db/seeders --migrations-path ./src/db/migrations",
    "seed:undo": "npx sequelize db:seed:undo:all --config ./src/db/config.js --models-path ./src/models --seeders-path ./src/db/seeders --migrations-path ./src/db/migrations",
    "generate": "ts-node --transpile-only src/schema"
  },
  "dependencies": {
    "@deepstream/client": "^5.0.7",
    "apollo-server-express": "2.11.0",
    "bcryptjs": "2.4.3",
    "crypto-js": "4.0.0",
    "dataloader-sequelize": "2.1.2",
    "date-fns": "2.11.0",
    "dotenv": "8.2.0",
    "graphql": "14.6.0",
    "graphql-iso-date": "3.6.1",
    "graphql-middleware": "4.0.2",
    "graphql-shield": "7.2.1",
    "jsonwebtoken": "8.5.1",
    "nexus": "0.12.0-rc.14",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "ramda": "0.27.0",
    "sequelize": "5.21.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/crypto-js": "3.1.43",
    "@types/graphql-iso-date": "3.3.3",
    "@types/jsonwebtoken": "8.3.8",
    "@types/node": "13.9.4",
    "@types/pg": "^7.14.3",
    "@types/ramda": "types/npm-ramda#dist",
    "create-nexus-type": "1.1.7",
    "sequelize-cli": "5.5.1",
    "ts-node": "8.8.1",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.8.3"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  }
}
